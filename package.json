{
  "name": "quick-new-file",
  "displayName": "Quick New File",
  "description": "Create a new file that's similar to this file: same package and language.",
  "publisher": "avive",
  "version": "1.3.0",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  ],
  "main": "./out/extension.js",
  "icon": "icon.png",
  "contributes": {
    "commands": [
      {
        "command": "quick-new-file.new-file",
        "title": "New similar file"
      }
    ],
    "keybindings": [
      {
        "command": "quick-new-file.new-file",
        "key": "ctrl+shift+n",
        "mac": "cmd+shift+n",
        "when": "editorTextFocus"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avivey/vscode-quick-new-file.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run test",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint && cp src/test/*.test out/test/",
    "lint": "eslint src --ext ts",
    "test": "jest out/test/"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.2.2",
    "@types/glob": "^8.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.0",
    "eslint": "^9.32.0",
    "glob": "11.0",
    "jest": "^29.7.0",
    "typescript": "^5.8.3"
  }
}
